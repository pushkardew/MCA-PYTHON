     "text": [
      "Unique & Sorted Items: ['apple', 'banana', 'orange']\n"
     ]
    }
   ],
   "source": [
    "unique_items = set(items)\n",
    "\n",
    "sorted_unique = sorted(list(unique_items))\n",
    "\n",
    "print(\"Unique & Sorted Items:\", sorted_unique)"
   ]
  }
