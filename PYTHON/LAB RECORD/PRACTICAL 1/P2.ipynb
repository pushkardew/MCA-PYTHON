   "source": [
    "# Vowel Counter\n",
    "\n",
    "s = input(\"Enter a string: \").lower()\n",
    "print(\"string is\",s)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "3d079872",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Number of vowels in 'leeladhar': 4\n"
     ]
    }
   ],
   "source": [
    "vowels = \"aeiou\"\n",
    "count = 0\n",
    "\n",
    "for char in s:\n",
    "    if char in vowels:\n",
    "        count += 1\n",
    "\n",
    "print(f\"Number of vowels in '{s}': {count}\")"
   ]
  }
