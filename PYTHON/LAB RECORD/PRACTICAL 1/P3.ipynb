   "source": [
    "# Reverse a Number\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "print(\"number is\",num)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "01b49b01",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Reverse of 297548 is 845792\n"
     ]
    }
   ],
   "source": [
    "original = num\n",
    "reversed_num = 0\n",
    "\n",
    "while num > 0:\n",
    "    digit = num % 10\n",
    "    reversed_num = reversed_num * 10 + digit\n",
    "    num = num // 10\n",
    "\n",
    "print(f\"Reverse of {original} is {reversed_num}\")"
   ]
  }
