   "source": [
    "import string\n",
    "\n",
    "text = input(\"Enter text: \").lower()\n",
    "print(\"Entered text:\")\n",
    "print(text)\n",
    "for ch in string.punctuation:\n",
    "    text = text.replace(ch, \"\")\n",
    "\n",
    "words = text.split()\n",
    "freq = {}\n",
    "\n",
    "for word in words:\n",
    "    freq[word] = freq.get(word, 0) + 1\n",
    "print(\"\\nOutput:\")\n",
    "print(freq)\n"
   ]
  }
